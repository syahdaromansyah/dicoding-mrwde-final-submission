{
  "name": "dicoding-mrwde-final-submission",
  "description": "The final submission of the 'Menjadi React Web Developer Expert' (Become an Expert React Web Developer) course on the Dicoding web platform about creating a forum app with React.js",
  "private": true,
  "type": "module",
  "author": "Syahda Romansyah <syahdarom@gmail.com>",
  "scripts": {
    "dev": "vite",
    "dev:msw": "vite --mode development:msw",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier -w \"**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx,json,yaml,yml,css,md,mdx}\"",
    "format-c": "prettier -c \"**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx,json,yaml,yml,css,md,mdx}\"",
    "test": "vitest",
    "test:no-watch": "vitest --watch=false",
    "cy:open": "cypress open",
    "e2e": "cypress run",
    "ci:test": "vitest --watch=false && start-server-and-test dev:msw http://localhost:5173 e2e",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "lint-staged": {
    "*.{js,cjs,mjs,jsx,ts,cts,mts,tsx}": "eslint",
    "*.{js,cjs,mjs,jsx,ts,cts,mts,tsx,json,yaml,yml,css,md,mdx}": "prettier -w"
  },
